{
  "name": "@hash_yield/contracts",
  "version": "0.0.0",
  "description": "Hash Yield contracts",
  "scripts": {
    "build": "scarb build",
    "build:scarb": "scarb build",
    "format": "scarb fmt",
    "format:check": "scarb fmt --check",
    "lint:cairo": "echo 'No Cairo linting available'",
    "test": "snforge test",
    "test:fork": "snforge test --filter fork_",
    "test:unit": "snforge test --filter 'test_' --ignore-filter 'fork_'",
    "devnet": "bun --cwd ../scripts run devnet",
    "devnet:fork": "bun --cwd ../scripts run devnet:fork",
    "deploy:mocks": "bun --cwd ../scripts run deploy:mocks",
    "deploy:vault": "bun --cwd ../scripts run deploy:vault",
    "deploy:all": "bun --cwd ../scripts run deploy:all",
    "faucet": "bun --cwd ../scripts run faucet",
    "check-devnet": "bun --cwd ../scripts run check-devnet",
    "sync-addresses": "bun --cwd ../scripts run src/sync-addresses.ts"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.0.3",
    "starknet": "8.5.4"
  }
}
